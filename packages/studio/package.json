{
  "name": "@luciformresearch/ragforge-studio",
  "version": "0.3.3",
  "type": "module",
  "description": "RagForge Studio - Setup, monitoring and graph exploration for RagForge",
  "author": "Lucie Defraiteur <luciedefraiteur@luciformresearch.com>",
  "license": "LRSL-1.1",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "ELECTRON_DISABLE_SANDBOX=1 electron-vite dev",
    "build": "electron-vite build",
    "preview": "ELECTRON_DISABLE_SANDBOX=1 electron-vite preview",
    "package": "electron-builder",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "package:linux": "electron-builder --linux",
    "package:all": "electron-builder -mwl"
  },
  "dependencies": {
    "@luciformresearch/ragforge": "^0.3.3",
    "@luciformresearch/ragforge-cli": "^0.3.3",
    "@reactflow/node-resizer": "^2.2.14",
    "dagre": "^0.8.5",
    "dockerode": "^4.0.2",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.468.0",
    "neo4j-driver": "^5.27.0",
    "react-markdown": "^9.0.1",
    "reactflow": "^11.11.4",
    "shiki": "^1.24.0"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.53",
    "@types/dockerode": "^3.3.31",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.23",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "electron-vite": "^2.3.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.7.0",
    "vite": "^5.4.0"
  },
  "build": {
    "appId": "com.luciformresearch.ragforge-studio",
    "productName": "RagForge Studio",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "icon": "assets/icon.png",
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
